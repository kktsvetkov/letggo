{
	"name": "kktsvetkov/letggo",
	"type": "library",
	"description": "Letggo PHP Feature Flag",
	"keywords": ["feature-toggle", "feature-flag", "debug-bar", "debugbar"],
	"license": "LGPL-3.0",
	"authors": [
		{
		    "name": "Kaloyan (KT) Tsvetkov",
		    "email": "kaloyan@kaloyan.info"
		}
	],

	"require":
	{
		"php": ">=7.4"
	},

	"autoload":
	{
		"psr-4":
		{
		    "Letggo\\": "src/"
	    	}
	},

	"require-dev":
	{
		"phpunit/phpunit": "^9.5 | ^7.5"
	},

	"autoload-dev":
	{
		"psr-4":
		{
			"Letggo\\Tests\\": "tests/"
		}
	}
}
